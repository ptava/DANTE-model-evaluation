Description
    Writes out values of fields interpolated to a specified list of points.
\*---------------------------------------------------------------------------*/

probes
{
    type            probes;
    libs            ("libsampling.so");
    enabled         yes;
    startTime       $../controlDict!foLfStart;
    endTime         $../controlDict!foLfEnd;
    executeControl  runTime;
    executeInterval $../controlDict!foLfDeltaT;
    writeControl    runTime;
    executeAtStart  false;
    writeInterval   $../controlDict!foLfDeltaT;
    setFormat       csv;
    interpolationScheme	cellPoint;
    fields          (UMeanMag);

