isoSurfaces2
{
    type            surfaces;
    enabled	        yes;
    libs            ("libsampling.so");
    timeStart       2.7;
    timeEnd         3.0;
    surfaceFormat   vtk;
    writeControl    timeStep;
    writeInterval   4;
    fields          (p);
    interpolationScheme cellPointFace;

    surfaces
    (
        q1
        {
            type            isoSurface;
            isoField        Q;
	        isoValue        1500;
            interpolate     true;
        }
        q3
        {
            type            isoSurface;
            isoField        Q;
	        isoValue        1850;
            interpolate     true;
        }
     );
}
