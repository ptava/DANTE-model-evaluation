FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       foamRun;

solver            incompressibleFluid;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           2.0;

deltaT            0.0005;

writeControl      runTime;

writeInterval     2.0;

purgeWrite        0;

writeFormat       binary;

writePrecision    6;

writeCompression  off;

timeFormat        general;

timePrecision     6;

adjustTimeStep    yes;

maxCo             2.5;

runTimeModifiable yes;

foStart           2.0;      // start of acquisition

foEnd             3.0;      // end of acquisition

foDeltaT          0.05;     // acquisition time

foDeltaThf        0.01;     // high frequency acuisition time

refinementDeltaT  4;        // timesteps between refinements

libs (
    "libatmosphericModels.so"
);

OptimisationSwitches
{
    fileModificationSkew     0.5;
    // fileModificationChecking timeStampMaster;
    // fileHandler              collated;
}

DebugSwitches
{
    // myrefiner    1;
}

