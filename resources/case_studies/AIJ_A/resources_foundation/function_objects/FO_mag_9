mag1
{
    type            mag;
    libs            ("libfieldFunctionObjects.so");
    field           UMean;
    result          UMeanMag;
    region          region0;
    enabled         true;
    log             true;
    startTime       $../controlDict!foLfStart;
    endTime         $../controlDict!foLfEnd;
    executeControl  runTime;
    executeInterval $../controlDict!foLfDeltaT;
    writeControl    writeTime;
    executeAtStart  true;
}

mag2
{
    type            mag;
    libs            ("libfieldFunctionObjects.so");
    field           wallShearStressMean;
    result          wallShearStressMeanMag;
    region          region0;
    enabled         true;
    log             true;
    startTime       $../controlDict!foLfStart;
    endTime         $../controlDict!foLfEnd;
    executeControl  runTime;
    executeInterval $../controlDict!foLfDeltaT;
    writeControl    writeTime;
    executeAtStart  true;
}


