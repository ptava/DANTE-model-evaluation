isoSurfaces1
{
    type                surfaces;
    enabled             yes;
    libs                ("libsampling.so");
    startTime           $../controlDict!foLfStart;
    endTime             $../controlDict!foLfEnd;
    surfaceFormat       vtk;
    writeControl        runTime;
    executeAtStart      true;
    writeInterval       $../controlDict!foLfDeltaT;
    fields              (nutnu);
    interpolationScheme cellPointFace;

    surfaces
    (
        q1
        {
            type            isoSurface;
            isoField        Q;
	        isoValue        1500;
            interpolate     true;
        }
        q3
        {
            type            isoSurface;
            isoField        Q;
	        isoValue        1850;
            interpolate     true;
        }
     );
}
