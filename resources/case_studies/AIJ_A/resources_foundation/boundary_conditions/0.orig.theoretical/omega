FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField    uniform 60;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            atmBoundaryLayerInletOmega;
        #include        "include/ABLConditions"
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           $internalField;
    }
    building
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        value           $internalField;
    }
    ground
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        value           $internalField;
    }
}

// ************************************************************************* //
