isoSurfaces
{
    type            surfaces;
    enabled	        yes;
    libs            ("libsampling.so");
    timeStart       1.3;
    timeEnd         2.0;
    surfaceFormat   vtk;
    writeControl    timeStep;
    writeInterval   4;
    fields          (nutnu);
    interpolationScheme cellPointFace;

    surfaces
    (
        q1
        {
            type            isoSurface;
            isoField        Q;
	        isoValue        1500;
            interpolate     true;
        }
        q3
        {
            type            isoSurface;
            isoField        Q;
	        isoValue        1850;
            interpolate     true;
        }
     );
}
