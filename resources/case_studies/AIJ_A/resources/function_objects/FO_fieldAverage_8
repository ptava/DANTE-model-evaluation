fieldAverage
{
    type            fieldAverage;
    libs            (fieldFunctionObjects);
    restartOnRestart    false;
    restartOnOutput     false;
    periodicRestart     false;
    region          region0;
    enabled         true;
    log             true;
    timeStart       1.3;
    timeEnd         2.0;
    executeControl  timeStep;
    writeControl    writeTime;

    fields
    (
        p
        {
            mean         on;
            prime2Mean   on;
            base         time;
        }
        U
        {
            mean         on;
            prime2Mean   on;
            base         time;
        }
        k
        {
            mean         on;
            prime2Mean   on;
            base         time;
        }
        wallShearStress
        {
            mean         on;
            prime2Mean   on;
            base         time;
        }
    );

}


