FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          0.1;
    }

    pFinal
    {
        $p;
        relTol 0;
    }

    "(U|k|omega)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-6;
        relTol          0.01;
    }
    "(U|k|omega)Final"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-05;
        relTol          0;
    }

}

PIMPLE
{
	residualControl
	{
		p
		{
			relTol		0;
			tolerance	1e-04;
		}
		"(U|h|omega|epsilon|k|e)"
		{
			relTol		0;
			tolerance	1e-05;
		}
	}
	outerCorrectorResidualControl
	{
		p
		{
			relTol		0;
			tolerance	1e-04;
		}
		"(U|h|omega|epsilon|k|e)"
		{
			relTol		0;
			tolerance	1e-05;
		}
	}
    consistent	    yes;
    nNonOrthogonalCorrectors 2;
    nOuterCorrectors 1;
    nCorrectors      2;
    pRefCell         0;
    pRefValue        0;

}

relaxationFactors
{
    fields
    {
        "p.*"                   1;
    }
    equations
    {
        "U.*"                   1;
        "(k|omega|epsilon)"     1;
    }
}

